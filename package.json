{
  "name": "product-hunt",
  "version": "0.1.1",
  "description": "Unofficial wrapper of the public product hunt API.",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "babel src --out-dir lib --source-maps",
    "test": "mocha --require babel-core/register",
    "cover": "babel-node node_modules/.bin/babel-istanbul cover _mocha",
    "lint": "eslint --color src/ test/ --ext .js",
    "clean": "rm -rf lib coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danillouz/product-hunt.git"
  },
  "keywords": [
    "product-hunt",
    "public",
    "api"
  ],
  "author": "Daniel Illouz",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/danillouz/product-hunt/issues"
  },
  "homepage": "https://github.com/danillouz/product-hunt#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-istanbul": "^0.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.9.0",
    "mocha": "^2.4.5",
    "nock": "^8.0.0",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "node-fetch": "^1.5.2"
  }
}
